<div id="WaveImageControll" class="carousel slide" data-ride="carousel" data-interval='false'>
  
  <div class="carousel-inner">
    <% @names.wave.each do |name| %>
      <% if name == @names.wave[0] %>
        <div class="carousel-item active">
      <% else %>
        <div class="carousel-item">
      <%end%>
      <%= image_tag("waves/" + name + ".jpg") %>
      </div>
    <%end%>
  </div>
  
  <a class="carousel-control-prev" href="#WaveImageControll" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next" href="#WaveImageControll" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>
