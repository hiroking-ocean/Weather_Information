<div id="ChartImageControll" class="carousel slide" data-ride="carousel" data-interval='false'>
  
  <div class="carousel-inner">
    <% @names.chart.each do |name| %>
      <% if name == @names.chart[0] %>
        <div class="carousel-item active target" id="m">
      <% else %>
        <div class="carousel-item" id="m">
      <%end%>
      <%= image_tag("charts/" + name + ".jpg") %>
      </div>
    <%end%>
  </div>

  <a class="carousel-control-prev" href="#ChartImageControll" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next" href="#ChartImageControll" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>


