<div id="TimeImageControll" class="carousel slide" data-ride="carousel" data-interval='false'>
  <div class="carousel-inner">
    <% @names.parse(@names.sorted).each_with_index do |value, i| %>
        <div class="carousel-item <%= add_class(i, "active") %>" id="m">
          <h1><%= value.strftime("%Y") %></h1>
          <h1><%= value.strftime("%m月%d日") %></h1>
          <h1><%= value.strftime("%H時%M分") %></h1>
      </div>
    <%end%>
  </div>
</div>